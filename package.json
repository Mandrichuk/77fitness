{
  "name": "77fitness",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:init": "dotenv -e .env.local -- npx prisma init",
    "prisma:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.local -- npx prisma db seed ",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:generate": "dotenv -e .env.local npx prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset --force",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@reduxjs/toolkit": "^2.2.3",
    "@stripe/stripe-js": "^3.4.1",
    "@types/node": "20.11.30",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "i18next": "^23.10.1",
    "next": "14.1.4",
    "next-cloudinary": "^6.3.0",
    "next-intl": "^3.9.5",
    "postcss": "8.4.38",
    "raw-body": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-intl": "^6.6.3",
    "react-redux": "^9.1.1",
    "react-slick": "^0.30.2",
    "react-toastify": "^10.0.5",
    "sass": "^1.72.0",
    "sharp": "^0.33.5",
    "slick-carousel": "^1.8.1",
    "stripe": "^15.7.0",
    "tailwindcss": "3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "5.4.3",
    "zod": "^3.23.4"
  },
